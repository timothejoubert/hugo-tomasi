{
  "id": "home_page",
  "label": "Home page",
  "repeatable": false,
  "status": true,
  "format": "custom",
  "json": {
    "Main": {
      "uid": { "type": "UID", "config": { "label": "Slug", "placeholder": "Le nom de ta page dans l'url" } },
      "title": { "type": "Text", "config": { "label": "Title", "placeholder": "Titre de la page" } },
      "header_media": { "type": "Link", "config": { "label": "Header media", "placeholder": "", "select": "media" } },
      "over_title": { "type": "Text", "config": { "label": "Over title", "placeholder": "" } },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "Description de la page",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,hyperlink"
        }
      },
      "excerpt": {
        "type": "StructuredText",
        "config": {
          "label": "Excerpt",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,hyperlink"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "introduction_block": { "type": "SharedSlice" },
            "project_push_block": { "type": "SharedSlice" },
            "project_listing_block": { "type": "SharedSlice" }
          }
        }
      }
    },
    "Meta data": {
      "meta_title": { "type": "Text", "config": { "label": "Meta title", "placeholder": "" } },
      "meta_description": { "type": "Text", "config": { "label": "Meta description", "placeholder": "" } },
      "meta_image": { "type": "Link", "config": { "label": "Meta image", "placeholder": "", "select": "media" } }
    }
  }
}
